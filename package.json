{
  "name": "json-schema-to-typescript-lite",
  "type": "module",
  "version": "14.0.0",
  "packageManager": "pnpm@8.15.5",
  "description": "Lite version of json-schema-to-typescript",
  "author": "Boris Cherny <boris@performancejs.com> (http://performancejs.com/)",
  "license": "MIT",
  "homepage": "https://github.com/bcherny/json-schema-to-typescript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcherny/json-schema-to-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/bcherny/json-schema-to-typescript/issues"
  },
  "keywords": [
    "json",
    "schema",
    "typescript",
    "compile",
    "transpile",
    "api",
    "interface",
    "typing",
    "share"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^11.5.4",
    "@types/json-schema": "^7.0.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.11.1",
    "@types/lodash": "^4.17.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@types/rimraf": "^4.0.5",
    "ava": "^4.2.0",
    "bumpp": "^9.4.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "json-schema-to-typescript-lite": "workspace:*",
    "lodash-es": "^4.17.21",
    "rimraf": "^5.0.5",
    "shx": "^0.3.4",
    "tsify": "^5.0.4",
    "typescript": "^5.4.3",
    "unbuild": "^2.0.0",
    "vitest": "^1.4.0"
  },
  "ava": {
    "files": [
      "./dist/test/test.js"
    ],
    "ignoredByWatcher": [
      "./src"
    ],
    "snapshotDir": "./test/__snapshots__"
  }
}
